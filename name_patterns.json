{
  "prefixes": [
    "^\\d{1,4}[a-z]{1,3}[-.]",
    "^(?:tt|tv|show)[-.]"
  ],
  "suffixes": [
    "[-.](?:sd|hd|720p|1080p|x264|aac|dtshd|bluray)",
    "[-.](?:web|webrip|hdtv|proper|internal)"
  ],
  "replacements": {
    "dots_to_spaces": true,
    "underscores_to_spaces": true,
    "dashes_to_spaces": true
  },
  "patterns": [
    "^(.*?)\\s*-\\s*s(\\d{1,2})e(\\d{1,2})\\s*-",
    "[._-][sS](\\d{1,2})[eE](\\d{1,2})"
  ]
}